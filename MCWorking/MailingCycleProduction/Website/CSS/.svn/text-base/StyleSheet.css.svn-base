/* CSS Document */

body {
	background-color: #ffcc00;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
	color: #000000;
	text-align: center;
	margin: 0px;
	padding: 0px;
}

select {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
	color: #000000;
}

input.textbox {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
	color: #000000;
}

img {
	border: 0px;
}

a {
	color: Blue;
	text-decoration: underline;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
}

a:visited {
	color: Blue;
	text-decoration: underline;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
}

a:hover {
	color: Blue;
	text-decoration: underline;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
}

#page-holder {
	width: 960px;
	margin: auto;
	text-align: left;
	background: #FFFFFF;
}

#header-container {
	width: 100%;
	height: 110px;
	background-color: #ffcc00;
}

#header-logo {
	width: 309px;
	float: left;
	height: 84px;
}

#header-content {
	width: 590px;
	float: right;
	height: 84px;
}

#primary-nav {
	width: 400px;
	float: left;
	background-color: #000000;
	height: 21px;
	color: #FFFFFF;
	font-weight: bold;
	padding-top: 5px;
	padding-left: 20px;
}

#primary-nav a {
	font-family: Arial, Helvetica, sans-serif;
	color: #FFFFFF;
	letter-spacing: 0.05em;
	text-decoration: none;
}

#primary-nav a:hover {
	font-family: Arial, Helvetica, sans-serif;
	color: #FFCC00;
	letter-spacing: 0.05em;
	text-decoration: none;
}

#login-nav {
	width: 400px;
	float: right;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
	height: 20px;
}

#page-title {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 20px;
	color: #FFFFFF;
	text-align: right;
	background-image: url(../images/top_bg.jpg);
	background-repeat: no-repeat;
	height: 52px;
	padding: 10px 20px 0px 0px;
}

/* left nav column
----------------------------------------------- */
#left-nav{ 
	width: 170px;
	float: left;
	overflow: hidden;
	padding: 16px 0px 0px 15px;
}

#left-nav-text{ 
	width: 170px;
	float: left;
	overflow: hidden;
	padding: 16px 0px 0px 20px;
}

#left-nav ul {
	list-style: none;
	margin: 0;
	padding: 0;
}

#left-nav ul li {
	margin: 0;
	padding: 0;
	position: relative;
}

#left-nav ul li a {
	display: block;
	padding: 6px 10px 5px;
	color: #000;
	text-align: left;
	text-decoration: none;
	font-size: 11px;
	font-weight: normal;
	width: 150px;
	height: 12px;
	background-color: #efefef;
	margin-top: 2px;
}

#left-nav ul li a:hover {
	background-color: #CC3300;
	font-weight: bold;
	color: #FFFFFF;
}

#left-nav #my-account
{
	background-color: #ffffff;
	position: absolute;
	top:1px;
	left: 168px;
	z-index: 1000;
	visibility:hidden;
}

#my-account ul li a:hover 
{
	background-color: #CC3300;
	font-weight: bold;
	color: #FFFFFF;
}

#left-nav #agent-reports
{
	background-color: #ffffff;
	position: absolute;
	top:1px;
	left: 168px;
	z-index: 1000;
	visibility:hidden;
}

#agent-reports ul li a:hover 
{
	background-color: #CC3300;
	font-weight: bold;
	color: #FFFFFF;
}
/*-----------------------------------------------*/

/* left nav dhtml menu
----------------------------------------------- */
#left-nav .vertical-menu {
	margin: 0;
	padding: 0;
	background-color: #ffffff;
}

#left-nav .vertical-menu .menu-item {
	margin: 0;
	padding: 0;
	position: relative;
}

#left-nav .vertical-menu .menu-item a {
	display: block;
	padding: 6px 10px 5px;
	color: #000;
	text-align: left;
	text-decoration: none;
	font-size: 11px;
	font-weight: normal;
	width: 160px;
	height: 12px;
	background-color: #efefef;
	margin-top: 2px;
}

#left-nav .vertical-menu .menu-item a:hover {
	background-color: #CC3300;
	font-weight: bold;
	color: #FFFFFF;
}

#left-nav .vertical-menu .menu-item .submenu {
	position: absolute;
	background-color: #ffffff;
	top: -50px;
	left: 170px;
	display: none;
}

#left-nav .vertical-menu .menu-item .submenularge {
	position: absolute;
	background-color: #ffffff;
	top: -100px;
	left: 170px;
	display: none;
}
/*-----------------------------------------------*/

