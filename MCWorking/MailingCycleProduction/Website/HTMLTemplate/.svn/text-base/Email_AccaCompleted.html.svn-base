<body>
    <div id="holder">
        <fieldset><legend class="LegendText">List of Successfull events</legend>
        <table border="0" cellspacing="0" cellpadding="0" width="100%">
            <tr>
                <td colspan="8">
                    &nbsp;</td>
            </tr>           
            <tr>                
                <td>
                   <table id="EventsTable" width="100%">
                   <thead>
                   <tr>
                    <th>Event id</th>
                    <th>Event Date</th>
                    <th>Farm Name</th>
                    <th>Plan Name</th>
